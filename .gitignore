# Add to .gitignore to protect sensitive files

# Environment files with secrets
.env
.env.local
.env.production
.env.staging
*.env

# Exclude example files (these should be committed)
!.env.example
!.env.production.example

# Security reports
security-reports/
*.audit.json

# SSL/TLS certificates and keys
*.pem
*.key
*.crt
*.csr
*.p12
*.pfx
*.jks
*.keystore
*.truststore

# Database files
*.db
*.sqlite
*.sqlite3

# Log files
*.log
logs/
log/

# Temporary files
*.tmp
*.temp
*.swp
*.swo
*~

# IDE files
.vscode/
.idea/
*.sublime-*
.atom/

# OS files
.DS_Store
Thumbs.db
desktop.ini

# Node.js
node_modules/
npm-debug.log*
yarn-debug.log*
yarn-error.log*
.npm
.yarn-integrity

# Python
__pycache__/
*.py[cod]
*$py.class
*.so
.Python
env/
venv/
ENV/
env.bak/
venv.bak/
.pytest_cache/
.coverage
htmlcov/

# Docker
.dockerignore

# Build outputs
dist/
build/
*.egg-info/

# Blockchain specific
wallet/
crypto/
organizations/
channel-artifacts/
*.block
*.tx

# Test files
test-results/
coverage/

# Backup files
*.bak
*.backup
*.old